{% block website_meta_title %}
    {% if title %}
        <meta name="title" content="{{ title }}">
    {% else %}
        <meta name="title" content="دسوق اون لاين">
    {% endif %}
{% endblock %}

{% block website_description %}
    {% if description %}
        <meta name="description" content="{{ description }}">
    {% else %}
        <meta name="description" content="دسوق اون لاين">
    {% endif %}
{% endblock %}