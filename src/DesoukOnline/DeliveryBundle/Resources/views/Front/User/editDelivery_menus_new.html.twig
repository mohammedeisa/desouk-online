{% extends 'base.html.twig' %}
{% block inner_content %}
<!-- Page Content -->
<section id="mall" class="">
    <div class="container">
        <div class="box">
            <div class="center">
                <h3>{{ delivery.title }}</h3>
            </div>
            <div class="row">
                <div class="col-md-12">
                   {{ include('DesoukOnlineDeliveryBundle:Front:User/userMenu.html.twig', { 'selected': 'menu' }) }}
		            {% for flashMessage in app.session.flashbag.get('success') %}
		            	<div class="alert alert-success">
						    <a href="#" class="close" data-dismiss="alert">&times;</a>
						    <strong>{{ flashMessage }}</strong> 
						</div>
					{% endfor %}
		            <div id="vendor_edit_form">
	                    <div class="raw">
	                    	<div class="user_new_form">
                        		{{ form_start(menu_form) }}
                        		{{ form_widget(menu_form) }}
								{{ form_end(menu_form) }}
                        	</div>
	                    </div>
						
		            </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.container -->
</section>
{% endblock %}